knitr::opts_chunk$set(echo = TRUE)
require(languageR, quietly = TRUE)
data(english)
young = english[english$AgeSubject == "young", ] # mladi
old = english[english$AgeSubject == "old", ] # stari
RTlexdec_young = young[, "RTlexdec"] # vrijeme potrebno mladima za rješavanje prvog zadatka
RTlexdec_old = old[, "RTlexdec"] # vrijeme potrebno starima za rješavanje prvog zadatka
RTnaming_young = young[, "RTnaming"] # vrijeme potrebno mladima za rješavanje drugog zadatka
RTnaming_old = old[, "RTnaming"] # vrijeme potrebno starima za rješavanje drugog zadatka
mean(RTlexdec_young)
mean(RTlexdec_old)
mean(RTnaming_young)
mean(RTnaming_old)
plot(RTlexdec_young, col = 'blue',
ylim = c(min(english$RTlexdec), max(english$RTlexdec)),
ylab = "Vrijeme za prvi zadatak")
points(RTlexdec_old, col='red')
plot(RTnaming_young, col = 'blue',
ylim = c(min(english$RTnaming), max(english$RTnaming)),
ylab = "Vrijeme za drugi zadatak")
points(RTnaming_old, col='red')
cor(english$Familiarity, english$WrittenFrequency)
plot(english$Familiarity, english$WrittenFrequency)
#plot(english$Familiarity, english$WrittenSpokenFrequencyRatio)
firstVowel = english[english$CV == "V",]
firstConsonant = english[english$CV == "C",]
mean(firstConsonant$Familiarity)
mean(firstVowel$Familiarity)
hist(firstConsonant$Familiarity)
hist(firstVowel$Familiarity)
var(firstConsonant$Familiarity)
var(firstVowel$Familiarity)
var.test(firstConsonant$Familiarity, firstVowel$Familiarity)
t.test(firstVowel$Familiarity, firstConsonant$Familiarity, alt = "greater", var.equal = TRUE)
#Znači testira san je li se ljudima poznatije riječi koje počinju sa samoglasnicima od onih koje počinju sa suglasnicima,histogrami su da bi se pokazala normalnost podataka šta više manje je. Šta se tiče testa nisan baš siguran kako se to interpterira al mislin da kaže da ne postoji razlika.
